groups{
	attribute-group attrgrp1 = {
		attribute attr3 { float }
	}
	attribute-group attrgrp2 = {
		attribute attr4 { boolean }
	}
}
grammar{
	//foo = {
		skip attribute * {name},
		empty
	}
	/blub = {
		lax attribute * {name1,name2}, attribute-group attrgrp1,
		empty
	}
	/baz = {
		attribute-group attrgrp2,
		attribute attr1 {
			Type fixed "node1"
		},
		attribute attr2 {
			Type default "node2"
		}?,
		empty
	}
    //bob = {
		attribute attr1 { Typ default "def"},
		empty
	}
	//bam = {
		attribute-group attrgrp1,
		empty
	}
}