default namespace http://example.org/namespace
	grammar {
		/root = {
			element first,
			element second,
			element third
		}
		/root/first = {
			element alpha,
			element beta
		}
		/root/second = {
			element alpha,
			element beta
		}
		/root/third = {
			element alpha,
			element beta
		}
		//alpha = {
			string
		}
		//beta = {
			string
		}
		/foo = {
			( element foo )+
			| element bar
		}
		(/foo)+/bar = {
			string
		}
		
	}

