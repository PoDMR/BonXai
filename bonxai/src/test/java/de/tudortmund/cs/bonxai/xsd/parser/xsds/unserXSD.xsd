    </xs:simpleType>
    <xs:simpleType name="gYear">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="gMonthDay">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="gDay">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
