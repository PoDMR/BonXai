    <xs:simpleType name="base64Binary">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="anyURI">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="QName">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
