    <xs:simpleType name="normalizedString">
        <xs:restriction base="string">
            <xs:whiteSpace fixed="false" value="replace"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="token">
        <xs:restriction base="normalizedString">
