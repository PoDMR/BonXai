<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:hfp="http://www.w3.org/2001/XMLSchema-hasFacetAndProperty" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/xml.xsd"/>
    
	<xs:complexType name="openAttrs">
        <xs:complexContent>
            <xs:restriction base="anyType">
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="annotated">
        <xs:complexContent>
            <xs:extension base="openAttrs">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                </xs:sequence>
                <xs:attribute name="id" type="ID"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:simpleType name="formChoice">
        <xs:restriction base="NMTOKEN">
            <xs:enumeration value="qualified"/>
            <xs:enumeration value="unqualified"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="reducedDerivationControl">
        <xs:restriction base="derivationControl">
            <xs:enumeration value="extension"/>
            <xs:enumeration value="restriction"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="derivationSet">
        <xs:union>
            <xs:simpleType>
                <xs:restriction base="token">
                    <xs:enumeration value="#all"/>
                </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
                <xs:list itemType="reducedDerivationControl"/>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>
    <xs:simpleType name="typeDerivationControl">
        <xs:restriction base="derivationControl">
            <xs:enumeration value="extension"/>
            <xs:enumeration value="restriction"/>
            <xs:enumeration value="list"/>
            <xs:enumeration value="union"/>
        </xs:restriction>
    </xs:simpleType>
	
    <xs:simpleType name="fullDerivationSet">
        <xs:union>
            <xs:simpleType>
                <xs:list itemType="typeDerivationControl"/>
            </xs:simpleType>
            <xs:simpleType>
                <xs:restriction base="token">
                    <xs:enumeration value="#all"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>
	

    <xs:simpleType name="allNNI">
        <xs:union>
            <xs:simpleType>
                <xs:restriction base="NMTOKEN">
                    <xs:enumeration value="unbounded"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>
	
	
    <xs:complexType name="attribute">
        <xs:complexContent>
            <xs:extension base="annotated">
                <xs:sequence>
                    <xs:element minOccurs="0" name="simpleType">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="simpleType">
                                    <xs:sequence>
                                        <xs:element minOccurs="0" ref="annotation"/>
                                        <xs:group ref="simpleDerivation"/>
                                    </xs:sequence>
                                    <xs:attribute name="name" type="anyType" use="prohibited"/>
                                    <xs:attribute name="final" type="anyType" use="prohibited"/>
                                    <xs:anyAttribute namespace="##other" processContents="lax"/>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attributeGroup ref="defRef"/>
                <xs:attribute name="type" type="QName"/>
                <xs:attribute default="optional" name="use" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="NMTOKEN">
                            <xs:enumeration value="prohibited"/>
                            <xs:enumeration value="optional"/>
                            <xs:enumeration value="required"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="default" type="string"/>
                <xs:attribute name="fixed" type="string"/>
                <xs:attribute name="form" type="formChoice"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
	
	
    <xs:complexType name="topLevelAttribute">
        <xs:complexContent>
            <xs:restriction base="attribute">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:element minOccurs="0" name="simpleType" type="localSimpleType"/>
                </xs:sequence>
                <xs:attribute name="ref" type="anyType" use="prohibited"/>
                <xs:attribute name="form" type="anyType" use="prohibited"/>
                <xs:attribute name="use" type="anyType" use="prohibited"/>
                <xs:attribute name="name" type="NCName" use="required"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="complexType">
        <xs:complexContent>
            <xs:extension base="annotated">
                <xs:group ref="complexTypeModel"/>
                <xs:attribute name="name" type="NCName"/>
                <xs:attribute default="false" name="mixed" type="boolean" use="optional"/>
                <xs:attribute default="false" name="abstract" type="boolean" use="optional"/>
                <xs:attribute name="final" type="derivationSet"/>
                <xs:attribute name="block" type="derivationSet"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="topLevelComplexType">
        <xs:complexContent>
            <xs:restriction base="complexType">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:group ref="complexTypeModel"/>
                </xs:sequence>
                <xs:attribute name="name" type="NCName" use="required"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="localComplexType">
        <xs:complexContent>
            <xs:restriction base="complexType">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:group ref="complexTypeModel"/>
                </xs:sequence>
                <xs:attribute name="name" type="anyType" use="prohibited"/>
                <xs:attribute name="abstract" type="anyType" use="prohibited"/>
                <xs:attribute name="final" type="anyType" use="prohibited"/>
                <xs:attribute name="block" type="anyType" use="prohibited"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="restrictionType">
        <xs:complexContent>
            <xs:extension base="annotated">
                <xs:sequence>
                    <xs:choice minOccurs="0">
                        <xs:group ref="typeDefParticle"/>
                        <xs:group ref="simpleRestrictionModel"/>
                    </xs:choice>
                    <xs:group ref="attrDecls"/>
                </xs:sequence>
                <xs:attribute name="base" type="QName" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="complexRestrictionType">
        <xs:complexContent>
            <xs:restriction base="restrictionType">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:choice minOccurs="0">
                        <xs:group ref="typeDefParticle"/>
                    </xs:choice>
                    <xs:group ref="attrDecls"/>
                </xs:sequence>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="extensionType">
        <xs:complexContent>
            <xs:extension base="annotated">
                <xs:sequence>
                    <xs:group ref="attrDecls"/>
                </xs:sequence>
                <xs:attribute name="base" type="QName" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="simpleRestrictionType">
        <xs:complexContent>
            <xs:restriction base="restrictionType">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:choice minOccurs="0">
                        <xs:group ref="simpleRestrictionModel"/>
                    </xs:choice>
                    <xs:group ref="attrDecls"/>
                </xs:sequence>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="simpleExtensionType">
        <xs:complexContent>
            <xs:restriction base="extensionType">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:group ref="attrDecls"/>
                </xs:sequence>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:simpleType name="blockSet">
        <xs:union>
            <xs:simpleType>
                <xs:restriction base="token">
                    <xs:enumeration value="#all"/> <xs:complexType name="element"
                </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
                <xs:list>
                    <xs:simpleType>
                        <xs:restriction base="derivationControl">
                            <xs:enumeration value="extension"/>
                            <xs:enumeration value="restriction"/>
                            <xs:enumeration value="substitution"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:list>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>
	
    <xs:complexType name="element">
        <xs:complexContent>
            <xs:extension base="annotated">
                <xs:sequence>
                    <xs:choice minOccurs="0">
                        <xs:element name="simpleType" type="localSimpleType"/>
                        <xs:element name="complexType" type="localComplexType"/>
                    </xs:choice>
                </xs:sequence>
                <xs:attributeGroup ref="defRef"/>
                <xs:attribute name="type" type="QName"/>
                <xs:attribute name="substitutionGroup" type="QName"/>
                <xs:attributeGroup ref="occurs"/>
                <xs:attribute name="default" type="string"/>
                <xs:attribute name="fixed" type="string"/>
                <xs:attribute default="false" name="nillable" type="boolean" use="optional"/>
                <xs:attribute default="false" name="abstract" type="boolean" use="optional"/>
                <xs:attribute name="final" type="derivationSet"/>
                <xs:attribute name="block" type="blockSet"/>
                <xs:attribute name="form" type="formChoice"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
	
	
    <xs:complexType name="topLevelElement">
        <xs:complexContent>
            <xs:restriction base="element">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:choice minOccurs="0">
                        <xs:element name="simpleType" type="localSimpleType"/>
                        <xs:element name="complexType" type="localComplexType"/>
                    </xs:choice>
                </xs:sequence>
                <xs:attribute name="ref" type="anyType" use="prohibited"/>
                <xs:attribute name="form" type="anyType" use="prohibited"/>
                <xs:attribute name="minOccurs" type="anyType" use="prohibited"/>
                <xs:attribute name="maxOccurs" type="anyType" use="prohibited"/>
                <xs:attribute name="name" type="NCName" use="required"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="localElement">
        <xs:complexContent>
            <xs:restriction base="element">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:choice minOccurs="0">
                        <xs:element name="simpleType" type="localSimpleType"/>
                        <xs:element name="complexType" type="localComplexType"/>
                    </xs:choice>
                </xs:sequence>
                <xs:attribute name="substitutionGroup" type="anyType" use="prohibited"/>
                <xs:attribute name="final" type="anyType" use="prohibited"/>
                <xs:attribute name="abstract" type="anyType" use="prohibited"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="group">
        <xs:complexContent>
            <xs:extension base="annotated">
                <xs:group maxOccurs="unbounded" minOccurs="0" ref="particle"/>
                <xs:attributeGroup ref="defRef"/>
                <xs:attributeGroup ref="occurs"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="realGroup">
        <xs:complexContent>
            <xs:restriction base="group">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:choice minOccurs="0">
                        <xs:element ref="all"/>
                        <xs:element ref="choice"/>
                        <xs:element ref="sequence"/>
                    </xs:choice>
                </xs:sequence>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="namedGroup">
        <xs:complexContent>
            <xs:restriction base="realGroup">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:choice>
                        <xs:element name="all">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:restriction base="all">
                                        <xs:group ref="allModel"/>
                                        <xs:attribute name="minOccurs" type="anyType" use="prohibited"/>
                                        <xs:attribute name="maxOccurs" type="anyType" use="prohibited"/>
                                        <xs:anyAttribute namespace="##other" processContents="lax"/>
                                    </xs:restriction>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="choice">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:restriction base="explicitGroup">
                                        <xs:sequence>
                                            <xs:element minOccurs="0" ref="annotation"/>
                                        </xs:sequence>
                                        <xs:attribute name="minOccurs" type="anyType" use="prohibited"/>
                                        <xs:attribute name="maxOccurs" type="anyType" use="prohibited"/>
                                        <xs:anyAttribute namespace="##other" processContents="lax"/>
                                    </xs:restriction>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="sequence" type="simpleExplicitGroup"/>
                    </xs:choice>
                </xs:sequence>
                <xs:attribute name="name" type="NCName" use="required"/>
                <xs:attribute name="ref" type="anyType" use="prohibited"/>
                <xs:attribute name="minOccurs" type="anyType" use="prohibited"/>
                <xs:attribute name="maxOccurs" type="anyType" use="prohibited"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="groupRef">
        <xs:complexContent>
            <xs:restriction base="realGroup">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                </xs:sequence>
                <xs:attribute name="ref" type="QName" use="required"/>
                <xs:attribute name="name" type="anyType" use="prohibited"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="explicitGroup">
        <xs:complexContent>
            <xs:restriction base="group">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                </xs:sequence>
                <xs:attribute name="name" type="NCName" use="prohibited"/>
                <xs:attribute name="ref" type="QName" use="prohibited"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="narrowMaxMin">
        <xs:complexContent>
            <xs:restriction base="localElement">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:choice minOccurs="0">
                        <xs:element name="simpleType" type="localSimpleType"/>
                        <xs:element name="complexType" type="localComplexType"/>
                    </xs:choice>
                </xs:sequence>
                <xs:attribute default="1" name="minOccurs" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="nonNegativeInteger">
                            <xs:enumeration value="0"/>
                            <xs:enumeration value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute default="1" name="maxOccurs" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="allNNI">
                            <xs:enumeration value="0"/>
                            <xs:enumeration value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="all">
        <xs:complexContent>
            <xs:restriction base="explicitGroup">
                <xs:group ref="allModel"/>
                <xs:attribute default="1" name="minOccurs" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="nonNegativeInteger">
                            <xs:enumeration value="0"/>
                            <xs:enumeration value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute default="1" name="maxOccurs" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="allNNI">
                            <xs:enumeration value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:complexType name="wildcard">
        <xs:complexContent>
            <xs:extension base="annotated">
                <xs:attribute default="##any" name="namespace" type="namespaceList" use="optional"/>
                <xs:attribute default="strict" name="processContents" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="NMTOKEN">
                            <xs:enumeration value="skip"/>
                            <xs:enumeration value="lax"/>
                            <xs:enumeration value="strict"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
	
    <xs:simpleType name="namespaceList">
        <xs:union>
            <xs:simpleType>
                <xs:list>
                    <xs:simpleType>
                        <xs:union>
                            <xs:simpleType>
                                <xs:restriction base="token">
                                    <xs:enumeration value="##targetNamespace"/>
                                    <xs:enumeration value="##local"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:union>
                    </xs:simpleType>
                </xs:list>
            </xs:simpleType>
            <xs:simpleType>
                <xs:restriction base="token">
                    <xs:enumeration value="##any"/>
                    <xs:enumeration value="##other"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>
    <xs:complexType name="attributeGroup">
        <xs:complexContent>
            <xs:extension base="annotated">
                <xs:group ref="attrDecls"/>
                <xs:attributeGroup ref="defRef"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="namedAttributeGroup">
        <xs:complexContent>
            <xs:restriction base="attributeGroup">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                    <xs:group ref="attrDecls"/>
                </xs:sequence>
                <xs:attribute name="name" type="NCName" use="required"/>
                <xs:attribute name="ref" type="anyType" use="prohibited"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="attributeGroupRef">
        <xs:complexContent>
            <xs:restriction base="attributeGroup">
                <xs:sequence>
                    <xs:element minOccurs="0" ref="annotation"/>
                </xs:sequence>
                <xs:attribute name="ref" type="QName" use="required"/>
                <xs:attribute name="name" type="anyType" use="prohibited"/>
                <xs:anyAttribute namespace="##other" processContents="lax"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="keybase">
        <xs:complexContent>
            <xs:extension base="annotated">
                <xs:sequence>
                    <xs:element ref="selector"/>
                    <xs:element maxOccurs="unbounded" ref="field"/>
                </xs:sequence>
                <xs:attribute name="name" type="NCName" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="public">
        <xs:restriction base="token"/>
    </xs:simpleType>
    <xs:simpleType name="anyType"/>
    <xs:simpleType name="string">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="false" value="preserve"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="boolean">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="float">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="double">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="decimal">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="duration">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="dateTime">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="time">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="date">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="gYearMonth">
        <xs:restriction base="anySimpleType">
            <xs:whiteSpace fixed="true" value="collapse"/>
