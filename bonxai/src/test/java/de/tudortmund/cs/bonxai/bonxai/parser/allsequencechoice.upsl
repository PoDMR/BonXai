default namespace http://example.org/namespace

grammar {
	/root = {
		element choice,
		element sequence,
		element all,
		element all2
	}

	/delta = {
		(element alpha)*,
		(element beta)+,
		(element gamma)?,
		(element delta)[3,7],
		(element epsilon)[23]
	}

	//choice = { element alpha | element beta | element gamma }
	//sequence = { element alpha , element beta, element gamma }
	//all = { element alpha? & element beta? }
	//all2 = { element alpha & element beta }

	//alpha = { string }
	//beta = { string }
	//gamma = { string }
	//delta = { string }
	//epsilon = { string }
}