<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <start>
    <ref name="a"/>
  </start>
  <define name="a">
    <grammar>
      <start>
        <parentRef name="b"/>
      </start>
    </grammar>
  </define>
  <define name="b">
    <element name="a">
      <empty/>
    </element>
  </define>
</grammar>
