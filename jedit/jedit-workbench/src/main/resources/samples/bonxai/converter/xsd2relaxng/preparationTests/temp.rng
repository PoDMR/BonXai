<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0" xmlns:hfp="http://www.w3.org/2001/XMLSchema-hasFacetAndProperty" xmlns:xs="http://www.w3.org/2001/XMLSchema" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" ns="http://www.w3.org/2001/XMLSchema">
  <start>
    <choice>
      <ref name="e_schema"/>
      <ref name="e_anyAttribute"/>
      <ref name="e_complexContent"/>
      <ref name="e_simpleContent"/>
      <ref name="e_complexType"/>
      <ref name="e_element"/>
      <ref name="e_all"/>
      <ref name="e_choice"/>
      <ref name="e_sequence"/>
      <ref name="e_group"/>
      <ref name="e_any"/>
      <ref name="e_attribute"/>
      <ref name="e_attributeGroup"/>
      <ref name="e_include"/>
      <ref name="e_redefine"/>
      <ref name="e_import"/>
      <ref name="e_selector"/>
      <ref name="e_field"/>
      <ref name="e_unique"/>
      <ref name="e_key"/>
      <ref name="e_keyref"/>
      <ref name="e_notation"/>
      <ref name="e_appinfo"/>
      <ref name="e_documentation"/>
      <ref name="e_annotation"/>
      <ref name="e_simpleType"/>
      <ref name="e_restriction"/>
      <ref name="e_list"/>
      <ref name="e_union"/>
      <ref name="e_minExclusive"/>
      <ref name="e_minInclusive"/>
      <ref name="e_maxExclusive"/>
      <ref name="e_maxInclusive"/>
      <ref name="e_totalDigits"/>
      <ref name="e_fractionDigits"/>
      <ref name="e_length"/>
      <ref name="e_minLength"/>
      <ref name="e_maxLength"/>
      <ref name="e_enumeration"/>
      <ref name="e_whiteSpace"/>
      <ref name="e_pattern"/>
    </choice>
  </start>
  <define name="e_schema">
    <element name="schema" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_openAttrs"/>
      <optional>
        <attribute name="targetNamespace">
          <data type="anyURI"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="version">
          <choice>
            <data type="token"/>
            <ref name="t_public"/>
            <data type="language"/>
            <ref name="t_reducedDerivationControl"/>
            <data type="NMTOKEN"/>
            <data type="NCName"/>
            <data type="ENTITY"/>
            <ref name="t_formChoice"/>
            <ref name="t_typeDerivationControl"/>
            <ref name="t_derivationControl"/>
            <data type="IDREF"/>
            <data type="ID"/>
            <data type="Name"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="finalDefault">
          <ref name="t_fullDerivationSet"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="blockDefault">
          <ref name="t_blockSet"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="attributeFormDefault">
          <ref name="t_formChoice"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="elementFormDefault">
          <ref name="t_formChoice"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="id">
          <data type="ID"/>
        </attribute>
      </optional>
      <ref name="a_lang"/>
      <group>
        <zeroOrMore>
          <choice>
            <ref name="e_include"/>
            <ref name="e_import"/>
            <ref name="e_redefine"/>
            <ref name="e_annotation"/>
          </choice>
        </zeroOrMore>
        <zeroOrMore>
          <group>
            <ref name="g_schemaTop"/>
            <zeroOrMore>
              <ref name="e_annotation"/>
            </zeroOrMore>
          </group>
        </zeroOrMore>
      </group>
    </element>
  </define>
  <define name="t_openAttrs">
    <group>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
    </group>
  </define>
  <define name="t_public">
    <data type="token"/>
  </define>
  <define name="t_reducedDerivationControl">
    <choice>
      <value type="string">substitution</value>
      <value type="string">extension</value>
      <value type="string">restriction</value>
      <value type="string">list</value>
      <value type="string">union</value>
    </choice>
  </define>
  <define name="t_formChoice">
    <choice>
      <value type="string">qualified</value>
      <value type="string">unqualified</value>
    </choice>
  </define>
  <define name="t_typeDerivationControl">
    <choice>
      <value type="string">substitution</value>
      <value type="string">extension</value>
      <value type="string">restriction</value>
      <value type="string">list</value>
      <value type="string">union</value>
    </choice>
  </define>
  <define name="t_derivationControl">
    <choice>
      <value type="string">substitution</value>
      <value type="string">extension</value>
      <value type="string">restriction</value>
      <value type="string">list</value>
      <value type="string">union</value>
    </choice>
  </define>
  <define name="t_fullDerivationSet">
    <choice>
      <value type="string">#all</value>
      <list>
        <zeroOrMore>
          <ref name="t_typeDerivationControl"/>
        </zeroOrMore>
      </list>
    </choice>
  </define>
  <define name="t_blockSet">
    <choice>
      <value type="string">#all</value>
      <list>
        <zeroOrMore>
          <ref name="t_derivationControl"/>
        </zeroOrMore>
      </list>
    </choice>
  </define>
  <define name="a_lang">
    <optional>
      <attribute name="lang">
        <choice>
          <value type="string"/>
          <data type="language"/>
        </choice>
      </attribute>
    </optional>
  </define>
  <define name="e_include">
    <element name="include" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_annotated"/>
      <attribute name="schemaLocation">
        <data type="anyURI"/>
      </attribute>
    </element>
  </define>
  <define name="t_annotated">
    <group>
      <ref name="t_openAttrs"/>
      <optional>
        <attribute name="id">
          <data type="ID"/>
        </attribute>
      </optional>
      <optional>
        <ref name="e_annotation"/>
      </optional>
    </group>
  </define>
  <define name="e_annotation">
    <element name="annotation" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_openAttrs"/>
      <optional>
        <attribute name="id">
          <data type="ID"/>
        </attribute>
      </optional>
      <zeroOrMore>
        <choice>
          <ref name="e_appinfo"/>
          <ref name="e_documentation"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="e_appinfo">
    <element name="appinfo" ns="http://www.w3.org/2001/XMLSchema">
      <mixed>
        <group>
          <zeroOrMore>
            <element>
              <anyName/>
              <ref name="anyType"/>
            </element>
          </zeroOrMore>
          <optional>
            <attribute name="source">
              <data type="anyURI"/>
            </attribute>
          </optional>
          <oneOrMore>
            <attribute>
              <anyName>
                <except>
                  <nsName ns="http://www.w3.org/2001/XMLSchema"/>
                </except>
              </anyName>
            </attribute>
          </oneOrMore>
        </group>
      </mixed>
    </element>
  </define>
  <define name="anyType">
    <zeroOrMore>
      <choice>
        <element>
          <anyName/>
          <ref name="anyType"/>
        </element>
        <text/>
        <attribute>
          <anyName/>
        </attribute>
      </choice>
    </zeroOrMore>
  </define>
  <define name="e_documentation">
    <element name="documentation" ns="http://www.w3.org/2001/XMLSchema">
      <mixed>
        <group>
          <zeroOrMore>
            <element>
              <anyName/>
              <ref name="anyType"/>
            </element>
          </zeroOrMore>
          <optional>
            <attribute name="source">
              <data type="anyURI"/>
            </attribute>
          </optional>
          <ref name="a_lang"/>
          <oneOrMore>
            <attribute>
              <anyName>
                <except>
                  <nsName ns="http://www.w3.org/2001/XMLSchema"/>
                </except>
              </anyName>
            </attribute>
          </oneOrMore>
        </group>
      </mixed>
    </element>
  </define>
  <define name="e_import">
    <element name="import" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_annotated"/>
      <optional>
        <attribute name="namespace">
          <data type="anyURI"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="schemaLocation">
          <data type="anyURI"/>
        </attribute>
      </optional>
    </element>
  </define>
  <define name="e_redefine">
    <element name="redefine" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_openAttrs"/>
      <attribute name="schemaLocation">
        <data type="anyURI"/>
      </attribute>
      <optional>
        <attribute name="id">
          <data type="ID"/>
        </attribute>
      </optional>
      <zeroOrMore>
        <choice>
          <ref name="e_annotation"/>
          <ref name="g_redefinable"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="g_redefinable">
    <group>
      <choice>
        <ref name="e_simpleType"/>
        <ref name="e_complexType"/>
        <ref name="e_group"/>
        <ref name="e_attributeGroup"/>
      </choice>
    </group>
  </define>
  <define name="e_simpleType">
    <element name="simpleType" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_topLevelSimpleType"/>
    </element>
  </define>
  <define name="t_topLevelSimpleType">
    <group>
      <attribute name="name">
        <choice>
          <data type="NCName"/>
          <data type="ENTITY"/>
          <data type="IDREF"/>
          <data type="ID"/>
        </choice>
      </attribute>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
      <group>
        <optional>
          <ref name="e_annotation"/>
        </optional>
        <ref name="g_simpleDerivation"/>
      </group>
    </group>
  </define>
  <define name="g_simpleDerivation">
    <group>
      <choice>
        <ref name="e_restriction"/>
        <ref name="e_list"/>
        <ref name="e_union"/>
      </choice>
    </group>
  </define>
  <define name="e_restriction">
    <element name="restriction" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_annotated"/>
      <optional>
        <attribute name="base">
          <data type="QName"/>
        </attribute>
      </optional>
      <ref name="g_simpleRestrictionModel"/>
    </element>
  </define>
  <define name="g_simpleRestrictionModel">
    <group>
      <group>
        <optional>
          <element name="simpleType" ns="http://www.w3.org/2001/XMLSchema">
            <ref name="t_localSimpleType"/>
          </element>
        </optional>
        <zeroOrMore>
          <ref name="g_facets"/>
        </zeroOrMore>
      </group>
    </group>
  </define>
  <define name="t_localSimpleType">
    <group>
      <attribute name="name">
        <notAllowed/>
      </attribute>
      <attribute name="final">
        <notAllowed/>
      </attribute>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
      <group>
        <optional>
          <ref name="e_annotation"/>
        </optional>
        <ref name="g_simpleDerivation"/>
      </group>
    </group>
  </define>
  <define name="g_facets">
    <group>
      <choice>
        <ref name="e_minExclusive"/>
        <ref name="e_minInclusive"/>
        <ref name="e_maxExclusive"/>
        <ref name="e_maxInclusive"/>
        <ref name="e_totalDigits"/>
        <ref name="e_fractionDigits"/>
        <ref name="e_length"/>
        <ref name="e_minLength"/>
        <ref name="e_maxLength"/>
        <ref name="e_enumeration"/>
        <ref name="e_whiteSpace"/>
        <ref name="e_pattern"/>
      </choice>
    </group>
  </define>
  <define name="e_minExclusive">
    <element name="minExclusive" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_facet"/>
    </element>
  </define>
  <define name="t_facet">
    <group>
      <ref name="t_annotated"/>
      <attribute name="value"/>
      <optional>
        <attribute name="fixed">
          <data type="boolean"/>
        </attribute>
      </optional>
    </group>
  </define>
  <define name="e_minInclusive">
    <element name="minInclusive" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_facet"/>
    </element>
  </define>
  <define name="e_maxExclusive">
    <element name="maxExclusive" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_facet"/>
    </element>
  </define>
  <define name="e_maxInclusive">
    <element name="maxInclusive" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_facet"/>
    </element>
  </define>
  <define name="e_totalDigits">
    <element name="totalDigits" ns="http://www.w3.org/2001/XMLSchema">
      <attribute name="value">
        <data type="positiveInteger"/>
      </attribute>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
      <optional>
        <ref name="e_annotation"/>
      </optional>
    </element>
  </define>
  <define name="e_fractionDigits">
    <element name="fractionDigits" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_numFacet"/>
    </element>
  </define>
  <define name="t_numFacet">
    <group>
      <attribute name="value">
        <choice>
          <data type="nonNegativeInteger"/>
          <data type="unsignedInt"/>
          <data type="unsignedLong"/>
          <data type="unsignedShort"/>
          <data type="unsignedByte"/>
          <data type="positiveInteger"/>
        </choice>
      </attribute>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
      <optional>
        <ref name="e_annotation"/>
      </optional>
    </group>
  </define>
  <define name="e_length">
    <element name="length" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_numFacet"/>
    </element>
  </define>
  <define name="e_minLength">
    <element name="minLength" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_numFacet"/>
    </element>
  </define>
  <define name="e_maxLength">
    <element name="maxLength" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_numFacet"/>
    </element>
  </define>
  <define name="e_enumeration">
    <element name="enumeration" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_noFixedFacet"/>
    </element>
  </define>
  <define name="t_noFixedFacet">
    <group>
      <attribute name="fixed">
        <notAllowed/>
      </attribute>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
      <optional>
        <ref name="e_annotation"/>
      </optional>
    </group>
  </define>
  <define name="e_whiteSpace">
    <element name="whiteSpace" ns="http://www.w3.org/2001/XMLSchema">
      <attribute name="value">
        <choice>
          <value type="string">preserve</value>
          <value type="string">replace</value>
          <value type="string">collapse</value>
        </choice>
      </attribute>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
      <optional>
        <ref name="e_annotation"/>
      </optional>
    </element>
  </define>
  <define name="e_pattern">
    <element name="pattern" ns="http://www.w3.org/2001/XMLSchema">
      <attribute name="value">
        <choice>
          <data type="string"/>
          <ref name="t_public"/>
          <data type="language"/>
          <ref name="t_reducedDerivationControl"/>
          <data type="normalizedString"/>
          <data type="NMTOKEN"/>
          <data type="NCName"/>
          <data type="ENTITY"/>
          <ref name="t_formChoice"/>
          <ref name="t_typeDerivationControl"/>
          <ref name="t_derivationControl"/>
          <data type="IDREF"/>
          <data type="token"/>
          <data type="ID"/>
          <data type="Name"/>
        </choice>
      </attribute>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
      <optional>
        <ref name="e_annotation"/>
      </optional>
    </element>
  </define>
  <define name="e_list">
    <element name="list" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_annotated"/>
      <optional>
        <attribute name="itemType">
          <data type="QName"/>
        </attribute>
      </optional>
      <optional>
        <element name="simpleType" ns="http://www.w3.org/2001/XMLSchema">
          <ref name="t_localSimpleType"/>
        </element>
      </optional>
    </element>
  </define>
  <define name="e_union">
    <element name="union" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_annotated"/>
      <optional>
        <attribute name="memberTypes">
          <list>
            <zeroOrMore>
              <data type="QName"/>
            </zeroOrMore>
          </list>
        </attribute>
      </optional>
      <zeroOrMore>
        <element name="simpleType" ns="http://www.w3.org/2001/XMLSchema">
          <ref name="t_localSimpleType"/>
        </element>
      </zeroOrMore>
    </element>
  </define>
  <define name="e_complexType">
    <element name="complexType" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_topLevelComplexType"/>
    </element>
  </define>
  <define name="t_topLevelComplexType">
    <group>
      <attribute name="name">
        <choice>
          <data type="NCName"/>
          <data type="ENTITY"/>
          <data type="IDREF"/>
          <data type="ID"/>
        </choice>
      </attribute>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
      <group>
        <optional>
          <ref name="e_annotation"/>
        </optional>
        <ref name="g_complexTypeModel"/>
      </group>
    </group>
  </define>
  <define name="g_complexTypeModel">
    <group>
      <choice>
        <ref name="e_simpleContent"/>
        <ref name="e_complexContent"/>
        <group>
          <optional>
            <ref name="g_typeDefParticle"/>
          </optional>
          <ref name="g_attrDecls"/>
        </group>
      </choice>
    </group>
  </define>
  <define name="e_simpleContent">
    <element name="simpleContent" ns="http://www.w3.org/2001/XMLSchema">
      <ref name="t_annotated"/>
      <choice>
        <element name="restriction" ns="http://www.w3.org/2001/XMLSchema">
          <ref name="t_simpleRestrictionType"/>
        </element>
        <element name="extension" ns="http://www.w3.org/2001/XMLSchema">
          <ref name="t_simpleExtensionType"/>
        </element>
      </choice>
    </element>
  </define>
  <define name="t_simpleRestrictionType">
    <group>
      <oneOrMore>
        <attribute>
          <anyName>
            <except>
              <nsName ns="http://www.w3.org/2001/XMLSchema"/>
            </except>
          </anyName>
        </attribute>
      </oneOrMore>
      <group>
        <optional>
          <ref name="e_annotation"/>
        </optional>
        <optional>
          <ref name="g_simpleRestrictionModel"/>
        </optional>
        <ref name="g_attrDecls"/>
      </group>
    </group>
  </define>
  <define name="g_attrDecls">
    <group>
      <group>
        <zeroOrMore>
          <choice>
            <element name="attribute" ns="http://www.w3.org/2001/XMLSchema">
              <ref name="t_attribute"/>
            </element>
            <element name="attributeGroup" ns="http://www.w3.org/2001/XMLSchema">
              <ref name="t_attributeGroupRef"/>
            </element>
          </choice>
        </zeroOrMore>
        <optional>
          <ref name="e_anyAttribute"/>
        </optional>
      </group>
    </group>
  </define>
  <define name="t_attribute">
    <group>
      <ref name="t_annotated"/>
      <ref name="ag_defRef"/>
      <optional>
        <attribute name="type">
          <data type="QName"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="use">
          <choice>
            <value type="string">prohibited</value>
            <value type="string">optional</value>
            <value type="string">required</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="default">
          <choice>
            <data type="string"/>
            <ref name="t_public"/>
            <data type="language"/>
            <ref name="t_reducedDerivationControl"/>
            <data type="normalizedString"/>
            <data type="NMTOKEN"/>
            <data type="NCName"/>
            <data type="ENTITY"/>
            <ref name="t_formChoice"/>
            <ref name="t_typeDerivationControl"/>
            <ref name="t_derivationControl"/>
            <data type="IDREF"/>
            <data type="token"/>
            <data type="ID"/>
            <data type="Name"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="fixed">
          <choice>
            <data type="string"/>
            <ref name="t_public"/>
            <data type="language"/>
            <ref name="t_reducedDerivationControl"/>
            <data type="normalizedString"/>
            <data type="NMTOKEN"/>
            <data type="NCName"/>
            <data type="ENTITY"/>
            <ref name="t_formChoice"/>
            <ref name="t_typeDerivationControl"/>
            <ref name="t_derivationControl"/>
            <data type="IDREF"/>
            <data type="token"/>
            <data type="ID"/>
            <data type="Name"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="form">
          <ref name="t_formChoice"/>
        </attribute>
      </optional>
      <optional>
        <element name="simpleType" ns="http://www.w3.org/2001/XMLSchema">
          <ref name="t_localSimpleType"/>
        </element>
      </optional>
    </group>
  </define>
  <define name="ag_defRef">
    <group>
      <optional>
        <attribute name="name">
          <choice>
            <data type="NCName"/>
            <data type="ENTITY"/>
            <data type="IDREF"/>
            <data type="ID"/>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="ref">
          <data type="QName"/>
        </attribute>
      </optional>
    </group>
  </define>
  <define name="t_attributeGroupRef">
    <group>
      <attribute name="ref">
